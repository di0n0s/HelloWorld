<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="netmind.com.helloworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".SegundaActividad">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" /> <!--Esta actividad es el main-->

                <category android:name="android.intent.category.LAUNCHER" /> <!--Esta actividad es el launcher-->
            </intent-filter>
        </activity>

    </application>

</manifest>